    <script src="{{ url_for('static', filename='assets/plugins/raphael/raphael-min.js') }}"></script>
    <script src="{{ url_for('static', filename='assets/plugins/morrisjs/morris.min.js') }}"></script>
    <!-- Chart JS -->
